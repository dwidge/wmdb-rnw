    "@nozbe/watermelondb": "^0.27.1",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^22.5.4",
    "expect": "^29.7.0",
    "ncp": "^2.0.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.2.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@9.7.1",
  "engines": {
    "node": ">=20"
