{
  "name": "@dwidge/wmdb-expo",
  "version": "0.0.8",
  "description": "A WatermelonDB adapter for Expo applications.",
  "repository": "github:dwidge/wmdb-expo",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
